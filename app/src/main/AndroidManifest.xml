<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppInetractiva"
        tools:targetApi="31">
        <activity
            android:name=".Ap3_Activity"
            android:exported="false"
            android:label="@string/title_activity_ap3"
            android:theme="@style/Theme.AppInetractiva" />
        <activity
            android:name=".Ap2_Activity"
            android:exported="false"
            android:label="@string/title_activity_ap2"
            android:theme="@style/Theme.AppInetractiva" />
        <activity
            android:name=".Ap1_Activity"
            android:exported="false"
            android:label="@string/title_activity_ap1"
            android:theme="@style/Theme.AppInetractiva" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppInetractiva">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>